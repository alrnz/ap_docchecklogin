{
  "name": "antwerpes/ap_docchecklogin",
  "description": "Official DocCheck Login Extension for Typo3 <= 10.0",
  "type": "typo3-cms-extension",
  "homepage": "https://github.com/antwerpes/ap_docchecklogin",
  "keywords": [
    "typo3",
    "cms",
    "login",
    "doccheck"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Thorsten Daners",
      "email": "thorsten.daners@antwerpes.com"
    },
    {
      "name": "Michael Paffrath",
      "email": "michael.paffrath@antwerpes.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Antwerpes\\ApDocCheckLogin\\": "Classes/"
    }
  },
  "require": {
    "typo3/cms-core": "<10.0",
    "typo3/cms-extbase": "<10.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
